        "class Human :\n",
        "  def __init__(self,name,age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(\"名前：\", self.name)\n",
        "    print(\"年齢：\", self.age)\n",
        "\n",
        "person = Human(\"太郎\",23)\n",
        "\n",
        "person.printinfo()\n",
        "\n",
        "person = Human(\"花子\",22)\n",
        "\n",
        "person.printinfo()"
